    RowBox[{
     RowBox[{"subgraph", " ", "=", " ", 
      RowBox[{"Subgraph", "[", 
       RowBox[{"fullGraph", ",", " ", "component"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"vertices", " ", "=", " ", 
      RowBox[{"VertexList", "[", "subgraph", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"edges", " ", "=", " ", 
      RowBox[{"EdgeList", "[", "subgraph", "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"motherCells", "=", " ", 
      RowBox[{"First", "/@", 
       RowBox[{"DeleteCases", "[", 
        RowBox[{
         RowBox[{"Tally", "[", 
          RowBox[{"Last", "/@", "edges"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"x_", ",", "t_"}], "}"}], ",", "1"}], "}"}]}], "]"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"divisions", "=", 
      RowBox[{"Select", "[", 
       RowBox[{"edges", ",", " ", 
        RowBox[{
         RowBox[{"MemberQ", "[", 
          RowBox[{"motherCells", ",", 
           RowBox[{"Last", "[", "#", "]"}]}], "]"}], "&"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"daughterCells", "=", " ", 
      RowBox[{"First", "/@", "divisions"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"singleEdges", " ", "=", " ", 
      RowBox[{"Select", "[", 
       RowBox[{"edges", ",", " ", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"MemberQ", "[", 
           RowBox[{"motherCells", ",", 
            RowBox[{"Last", "[", "#", "]"}]}], "]"}]}], "&"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"tracks", " ", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"SortBy", "[", 
         RowBox[{"#", ",", "Last"}], "]"}], "&"}], "/@", " ", 
       RowBox[{"ConnectedComponents", "[", 
        RowBox[{"Graph", "[", "singleEdges", "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "tracks"}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",
