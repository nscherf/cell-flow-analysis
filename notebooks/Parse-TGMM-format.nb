 RowBox[{
  RowBox[{"parseGenealogy", "[", 
   RowBox[{"component_", ",", "fullGraph_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
