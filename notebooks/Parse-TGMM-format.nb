Cell[BoxData[{
 RowBox[{"Clear", "[", "parseLine", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"parseLine", "[", 
   RowBox[{"{", 
    RowBox[{
    "id_", ",", "motherID_", ",", "x_", ",", "y_", ",", "z_", ",", "r_", ",", 
